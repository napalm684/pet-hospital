{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Warning",
      "BluePaw.Router": "Information"
    }
  },
  "Spring": {
    "Application": {
      "Name": "administrationservice"
    }
  },
  "Cloud": {
    "Config": {
      "Uri": "http://configserver:8888",
      "FailFast": true,
      "ValidateCertificates": false
    }
  }
}